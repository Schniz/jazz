{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": ["GRAFANA_SERVICE_ACCOUNT"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["build"]
    }
  }
}
